    <PublisherName>Serkan Karışan</PublisherName>
  </PropertyGroup>
  <ItemGroup>
    <BootstrapperPackage Include="Microsoft.NetCore.DesktopRuntime.8.0.x64">
      <Install>true</Install>
      <ProductName>.NET Desktop Runtime 8.0.0 (x64)</ProductName>
    </BootstrapperPackage>
  </ItemGroup>
</Project>